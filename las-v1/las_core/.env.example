# LAS Core Environment Variables
# Copy this file to .env and fill in your actual values

# ============================================================
# LLM Provider API Keys
# ============================================================

# Ollama Cloud API Key (get from https://ollama.com/settings/keys)
OLLAMA_API_KEY=your_ollama_cloud_api_key_here

# OpenRouter API Key (get from https://openrouter.ai/settings/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google Gemini API Key (get from https://aistudio.google.com/app/apikey)
GOOGLE_API_KEY=your_google_api_key_here

# ============================================================
# Database Configuration
# ============================================================

POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=las_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# ============================================================
# Vector Database (Qdrant)
# ============================================================

QDRANT_HOST=localhost
QDRANT_PORT=6333

# ============================================================
# Search Engine (SearxNG)
# ============================================================

SEARXNG_BASE_URL=http://127.0.0.1:8080

# ============================================================
# LLM Provider Configuration
# ============================================================

PROVIDER_NAME=ollama
PROVIDER_MODEL=gpt-oss:20b-cloud
PROVIDER_SERVER_ADDRESS=http://127.0.0.1:11434

# ============================================================
# Application Settings
# ============================================================

IS_LOCAL=true
AGENT_NAME=Friday
WORK_DIR=./workspace
HEADLESS_BROWSER=false
STEALTH_MODE=false

# ============================================================
# Security
# ============================================================

# API Key for authentication (generate a secure random string)
LAS_API_KEY=your_secure_api_key_here

# ============================================================
# Optional: Additional Provider Keys
# ============================================================

# OPENAI_API_KEY=your_openai_key_here
# ANTHROPIC_API_KEY=your_anthropic_key_here
# DEEPSEEK_API_KEY=your_deepseek_key_here
# TOGETHER_API_KEY=your_together_key_here
# HUGGINGFACE_API_KEY=your_huggingface_key_here